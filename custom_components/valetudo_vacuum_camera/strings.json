{
  "domain": "camera",
  "name": "Valetudo Vacuum Camera",
  "documentation": "https://github.com/sca075/valetudo_vacuum_camera",
  "dependencies": [
    "mqtt"
  ],
  "entities": [
    {
      "entity_namespace": "valetudo_vacuum_camera",
      "entity_name": "{device_name} Map",
      "camera_stream": true,
      "input_boolean": true,
      "services": [
        {
          "service": "start",
          "name": "Start Cleaning",
          "description": "Start cleaning the house",
          "fields": []
        },
        {
          "service": "stop",
          "name": "Stop Cleaning",
          "description": "Stop the cleaning operation",
          "fields": []
        }
      ],
      "icon": "mdi:robot-vacuum"
    }
  ],
  "config_flow": true,
  "quality_scale": "20",
  "max_fps": "15"
}

